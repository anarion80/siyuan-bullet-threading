{
  "name": "siyuan-bullet-threading",
  "version": "0.1.5",
  "type": "module",
  "description": "A plugin that adds nice lines to nested list elements (bullets). Based on simliar plugin for LogSeq.",
  "repository": "https://github.com/anarion80/siyuan-bullet-threading",
  "homepage": "https://github.com/anarion80/siyuan-bullet-threading",
  "author": "anarion80",
  "license": "MIT",
  "scripts": {
    "dev": "cross-env NODE_ENV=development VITE_SOURCEMAP=inline vite build --watch",
    "build": "cross-env NODE_ENV=production vite build",
    "make-link": "node --no-warnings ./scripts/make_dev_link.js",
    "make-link-win": "powershell.exe -NoProfile -ExecutionPolicy Bypass -File ./scripts/elevate.ps1 -scriptPath ./scripts/make_dev_link.js",
    "update-version": "node --no-warnings ./scripts/update_version.js",
    "make-install": "vite build && node --no-warnings ./scripts/make_install.js"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^22.13.9",
    "cross-env": "^7.0.3",
    "fast-glob": "^3.2.12",
    "glob": "^11.0.1",
    "minimist": "^1.2.8",
    "rollup-plugin-livereload": "^2.0.5",
    "sass": "^1.63.3",
    "siyuan": "1.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.3",
    "vite": "^6.2.1",
    "vite-plugin-static-copy": "^2.3.0",
    "vite-plugin-zip-pack": "^1.0.5"
  },
  "dependencies": {
    "js-yaml": "^4.1.0"
  }
}